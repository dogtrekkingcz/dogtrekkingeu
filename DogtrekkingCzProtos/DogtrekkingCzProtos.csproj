      <Protobuf Include="Entities\Note.proto" Access="Public" GrpcServices="Both" ProtoCompile="True">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        <OutputDir>obj\Debug\net7.0\</OutputDir>
        <CompileOutputs>True</CompileOutputs>
        <Generator>MSBuild:Compile</Generator>
      </Protobuf>
      <Protobuf Include="Entities\Racer.proto" Access="Public" GrpcServices="Both" ProtoCompile="True">
