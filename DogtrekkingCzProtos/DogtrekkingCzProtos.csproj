<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="Address.proto" />
      <None Remove="Contact.proto" />
      <None Remove="Dog.proto" />
      <None Remove="Entities\ActionDetail.proto" />
      <None Remove="Entities\ActionSimple.proto" />
      <None Remove="Entities\Category.proto" />
      <None Remove="Entities\Note.proto" />
      <None Remove="Entities\RaceDetail.proto" />
      <None Remove="Entities\Racer.proto" />
      <None Remove="Entities\RaceSimple.proto" />
      <None Remove="Entities\UserProfile.proto" />
    </ItemGroup>

    <ItemGroup>
      <Protobuf Include="Entities\ActionDetail.proto" Access="Public" GrpcServices="Both" ProtoCompile="True">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        <OutputDir>obj\Debug\net7.0\</OutputDir>
        <CompileOutputs>True</CompileOutputs>
        <Generator>MSBuild:Compile</Generator>
      </Protobuf>
      <Protobuf Include="Entities\RaceDetail.proto" Access="Public" GrpcServices="Both" ProtoCompile="True">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        <OutputDir>obj\Debug\net7.0\</OutputDir>
        <CompileOutputs>True</CompileOutputs>
        <Generator>MSBuild:Compile</Generator>
      </Protobuf>
      <Protobuf Include="Entities\RaceSimple.proto" Access="Public" GrpcServices="Both" ProtoCompile="True">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        <OutputDir>obj\Debug\net7.0\</OutputDir>
        <CompileOutputs>True</CompileOutputs>
        <Generator>MSBuild:Compile</Generator>
      </Protobuf>
      <Protobuf Include="Entities\ActionSimple.proto" Access="Public" GrpcServices="Both" ProtoCompile="True">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
